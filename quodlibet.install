post_install() {
  echo "Updating icon cache"
  gtk-update-icon-cache -q -f -t /usr/share/icons/hicolor
}

post_upgrade() {
  echo "Updating icon cache"
  gtk-update-icon-cache -q -f -t /usr/share/icons/hicolor
}

post_remove() {
  echo "Updating icon cache"
  gtk-update-icon-cache -q -f -t /usr/share/icons/hicolor
}
